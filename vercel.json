{
    "version": 2,
    "functions": {
      "api/app.py": {
        "runtime": "@vercel/python",
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
